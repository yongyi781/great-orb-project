{
  /* See all the pre-defined configs here: https://www.npmjs.com/package/eslint-config-defaults */
  "env": { "browser": true },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      "warn",
      4
    ],
    "quotes": [
      "warn",
      "double"
    ],
    "semi": [
      "error",
      "always"
    ]
  },
  "globals": {
    "$": true,
    "Tone": true
  }
}